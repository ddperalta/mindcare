# Firebase Web App configuration
# Get these from Firebase Console → Project Settings → Your apps → Web app
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=1:your_sender_id:web:your_app_hash

# Encryption key for sensitive data (generate a strong random 32-char string)
VITE_ENCRYPTION_KEY=your_32_character_random_encryption_key

# Environment
VITE_ENV=development

# Optional: set to 'true' to use Firebase local emulators instead of production
VITE_USE_EMULATORS=false

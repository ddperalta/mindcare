{
  "name": "mental-health-saas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:functions": "cd functions && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "npm run build:functions && firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore:rules,firestore:indexes",
    "deploy:all": "npm run build && npm run build:functions && firebase deploy",
    "firebase:emulators": "firebase emulators:start",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "seed:tests": "node scripts/seedTestDefinitions.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "crypto-js": "^4.2.0",
    "firebase": "^12.9.0",
    "firebase-admin": "^13.6.1",
    "i18next": "^25.8.6",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
